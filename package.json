{
  "name": "liquachat",
  "version": "0.1.0",
  "description": "Open Source alternative to ChatGPT with multi-model support",
  "author": "Simone Cervini <simone@simonecervini.dev>",
  "license": "MIT",
  "homepage": "https://github.com/simonecervini/liquachat#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonecervini/liquachat.git"
  },
  "bugs": {
    "url": "https://github.com/simonecervini/liquachat/issues"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run scripts/db-seed.ts",
    "dev:next": "next dev --turbo",
    "dev:db": "./start-database.sh",
    "dev:zero": "zero-cache-dev -p src/zero/schema.ts",
    "zero:generate": "drizzle-zero generate --format --output src/zero/schema.gen.ts --schema src/server/db/schema.ts",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "cloc": "cloc --vcs=git --not-match-f='(pnpm-lock.yaml|yarn.lock|package-lock.json)'"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^2.2.8",
    "@base-ui-components/react": "1.0.0-beta.0",
    "@openrouter/ai-sdk-provider": "^0.7.1",
    "@rocicorp/zero": "^0.20.2025052100",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-form": "^1.12.2",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "ai": "^4.3.16",
    "bun": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dequal": "^2.0.3",
    "drizzle-orm": "^0.41.0",
    "drizzle-zero": "^0.10.5",
    "lorem-ipsum": "^2.0.8",
    "lucide-react": "^0.513.0",
    "motion": "^12.16.0",
    "next": "^15.2.3",
    "ollama-ai-provider": "^1.2.0",
    "postgres": "^3.4.4",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-aria-components": "^1.10.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-scan": "^0.3.4",
    "react-stately": "^3.39.0",
    "react-twc": "^1.4.2",
    "server-only": "^0.0.1",
    "shiki": "^3.6.0",
    "sonner": "^2.0.5",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/bun": "^1.2.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@8.15.5"
}
